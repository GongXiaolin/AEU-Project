ML{*
datatype TE = Tvar of int
| Tnum
| Tbool
| Tfun of (TE list * TE)
| Noty
*}

ML{*
val s = ‚Äústate‚Äù
val v = [(‚Äúcurvar‚Äù,‚Äúint‚Äù),(‚Äúvars‚Äù,"(string * TE)list"),(‚Äúeqns‚Äù,"(TE * TE) list")]
val _ = record_defs ["'a"] s v
}
ML{
fun make_tvarM v = (get_varsM‚Äìget_curvarM): |‚Äì(fn (vars,curvar)=>(set_varsM ((v,Tvar curvar)::vars))|-- set_curvarM (curvar+1)|-- returnM (Tvar curvar))
}
ML{
fun tb_defined v vars = if member v (fst (unzip vars)) then true else false
val c = tb_defined 3 [(1,2),(2,3),(3,4)]
}
ML{
fun tb_lookup v vars = snd(hd(filter(fn n =>(fst n) = v ) vars ))
val c = tb_lookup 3 [(3,4),(2,8)]
}
ML{
fun tb_lookup v ((m,n)::xs)= if v = m then n else tb_lookup v xs
}
ML{
fun varM v = get_varsM : |-- (fn vars => if (tb_defined v vars) then returnM (tb_lookup v vars)
else make_tvarM v)
}
ML{
fun eqnM (t1, t2) = get_eqnsM : |-- (fn eqns => set_eqnsM ((t1,t2)::eqns))
}
ML{
fun E (Const n) = returnM (Tnum)
|E (Var n) = varM (n)
|E (True) = returnM (Tbool)
|E (False) = returnM (Tbool)
|E (Add (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tnum))
|E(Minus(e1,e2)) = (E e1 ‚Äì E e2): |‚Äì(fn (t1,t2)=> eqnM(t1,Tnum)|‚ÄìeqnM(t2,Tnum)|‚ÄìreturnM (Tnum))
|E(Mult (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM(t1,Tnum)|‚ÄìeqnM(t2,Tnum)|‚ÄìreturnM (Tnum))
|E(Div (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM(t1,Tnum)|‚ÄìeqnM(t2,Tnum)|‚ÄìreturnM (Tnum))
|E(Neg e) = (E e): |-- (fn t => eqnM (t,Tnum)|‚ÄìreturnM(Tnum))
|E (And (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tbool) |-- eqnM (t2,Tbool) |-- returnM(Tbool))
|E (Or (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tbool) |-- eqnM (t2,Tbool) |-- returnM(Tbool))
|E(Not e) = (E e): |-- (fn t => eqnM (t,Tbool)|‚ÄìreturnM(Tbool))
|E (Lt (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))
|E (Le (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))
|E (Ge (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))
|E (Gt (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))
|E (Eq (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))
|E (Neq (e1,e2)) = (E e1 ‚Äì E e2): |-- (fn (t1,t2) => eqnM (t1,Tnum) |-- eqnM (t2,Tnum) |-- returnM(Tbool))

}
ML{
val (result_T, state {vars, eqns, ‚Ä¶}) =
( ‚Äú(x + y) && 20‚Äù |>tokens_of |>bexp |> fst |> E)
(state {vars = [], eqns = [], curvar = 0})
*}

ML{*
fun chaseM (t as Tvar _) = getM : |-- (fn tenv =>
if (tb_defined t tenv) then chaseM (tb_lookup t tenv) else returnM t)
| chaseM t = returnM t
*}

