14
and bterm x = ((bfactor ‚Äì repeat ((one(fn n => n =Delim ‚Äú&&‚Äù))-- bfactor))
>>(fn (e,ops) => foldl (fn ((Delim ‚Äú&&‚Äù,e2),e1) => And (e1,e2)) e ops ))x

and bexp x = ((bterm ‚Äì repeat ((one(fn n => n=Delim ‚Äú||‚Äù))‚Äìbterm)) >>
(fn (e,ops) => foldl (fn ((Delim ‚Äú||‚Äù,e2),e1) => Or (e1,e2)) e ops)) x
}
