fun FUND1 x = ((one(is_ident) ‚Äì (one (is_lb) |‚Äì(( (one (is_rb)|-- (one(is_LB)|-- SEQ --| one(is_RB))))) ))>> (fn (Ident a,b)=> Fdecl (( a,[]),b))) x
fun FUND2 x = ((one (is_ident) ‚Äì (one (is_lb) |-- add_ident‚Äì(one (is_rb) |-- (one (is_LB) |-- SEQ --|one (is_RB)))))>> (fn (Ident a,((Ident d,b),c)) => Fdecl ((a,(d::(map (peal) b))),c))) x
fun FUND x = (FUND1|| FUND2) x
}
