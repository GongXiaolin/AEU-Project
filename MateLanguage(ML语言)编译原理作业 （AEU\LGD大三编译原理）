ML{*
val totrue = one(fn Keyword ‚Äútrue‚Äù => true ) >> (fn n => True)
val tofalse = one(fn Keyword ‚Äúfalse‚Äù => true) >> (fn n => False)
val cop = one (fn n => n = Delim ‚Äú<‚Äù)
||one (fn n => n = Delim ‚Äú<=‚Äù)
||one (fn n => n = Delim ‚Äú>‚Äù)
||one (fn n => n = Delim ‚Äú>=‚Äù)
||one (fn n => n = Delim ‚Äú==‚Äù)
||one (fn n => n = Delim ‚Äú<>‚Äù)
fun tonot (Keyword ‚Äúnot‚Äù) = true
|tonot _ = false
}
ML{
fun bfactor x =((totrue) || (tofalse)

          ||(( aexp -- cop -- aexp)>>
                                     (
                                       fn ((a,Delim "<"),b) => Lt (a,b)
                                      |  ((a,Delim "<="),b) => Le (a,b)
                                      |  ((a,Delim ">"),b) => Gt (a,b)
                                      |  ((a,Delim ">="),b) => Ge (a,b)
                                      |  ((a,Delim "=="),b) => Eq (a,b)
                                      |  ((a,Delim "<>"),b) => Neq (a,b)
                                       ))
         
            || aexp 
             || ( (one is_lb |-- bexp --| one is_rb))
            || (((one tonot) |-- bfactor) >> (fn n =>Not n))
          ) x
1
