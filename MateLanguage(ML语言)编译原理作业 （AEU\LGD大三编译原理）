ML{*

val is_end = one (fn n => n= Delim ‚Äú;‚Äù)
val is_if = one (fn n => n = Keyword ‚Äúif‚Äù)
val is_then = one (fn n=> n = Keyword ‚Äúthen‚Äù)
val is_else = one (fn n=> n = Keyword ‚Äúelse‚Äù)
val is_while = one (fn n=> n = Keyword ‚Äúwhile‚Äù)
fun is_ident (Ident _) = true
|is_ident _ = false
fun is_equ (Delim ‚Äú=‚Äù) = true
|is_equ _ = false
fun is_LB (Delim ‚Äú{‚Äù) = true
|is_LB _ = false
fun is_RB (Delim ‚Äú}‚Äù) = true
|is_RB _ = false
val v = one (is_ident) ([Ident ‚Äúc‚Äù])
val jump = one (fn n=> true)
val dou = one (fn n => n = Delim ‚Äú;‚Äù)
fun peal( Ident x) = x
val c = jump [‚Äúa‚Äù]
val add = one (fn n=> n =Delim"+")
val sub = one (fn n=> n =Delim"-")
val mult = one (fn n=> n =Delim"*")
val chu = one (fn n=> n =Delim"/")
*}

ML{*
fun is_lb (Delim ‚Äú(‚Äù) = true
|is_lb _ = false
fun is_rb (Delim ‚Äú)‚Äù) = true
|is_rb _ = false
fun IFE x = ((((((is_if |-- bexpN --|is_then) ‚Äì ACMD) --|is_else) --ACMD ) ) >> (fn ((a,b),c)=> Ife (a,b,c))) x
and IFT x = ((((is_if |-- bexpN --| is_then) ‚Äì ACMD) ) >> (fn (a,b) => Ift (a,b))) x
and whilec x = ((((is_while |-- bexpN )-- ACMD) ) >> (fn (a,b)=> While (a,b))) x
and ASGN x = (((one (is_ident) --| (one (is_equ))) ‚Äì bexpN) >> (fn ((Ident a),b)=> Asgn (a,b))) x
and ACMD x = ( IFE || IFT
|| whilec

        ||FUNU
        || ASGN
        || RETURN
        
        ||BLOCK
        || (one (is_LB)) |-- SEQ --| one (is_RB)) x
1
2
3
4
5
6
(and SEQ x = (((ACMD ‚Äì (repeat (is_end |-- ACMD))) >> ((fn (e,ops)=> (foldl (fn (e1,e2)=> Seq (e1,e2)) e ops)))
)|| (ACMD >> (fn e => Seq(e,e) ))) x)
and SEQ x = (((ACMD ‚Äì (repeat (is_end |-- ACMD))) >> (fn (e,ops)=> if ops = [] then e else (foldl(fn (e1,e2)=> Seq (e1,e2)) (last ops) (rev(e::(but_last ops)))))) ) x
and PROG x = ( ( repeat FUND) >> (fn (b) => (Prog b))) x
and add_ident x =((one(is_ident) ‚Äì repeat (one (fn n => n= Delim ‚Äú,‚Äù)|‚Äìone (is_ident)))) x
and FUND1 x = ((one(is_ident) ‚Äì (one (is_lb) |‚Äì(( (one (is_rb)|‚ÄìBLOCK))) ))>> (fn (Ident a,b)=> Fdecl (( a,[]),b))) x
and FUND2 x = ((one (is_ident) ‚Äì (one (is_lb) |-- add_ident‚Äì(one (is_rb) |-- (BLOCK))))>> (fn (Ident a,((Ident d,b),c)) => Fdecl ((a,(d::(map (peal) b))),c))) x
and FUND x = (FUND1|| FUND2) x
and dexp x = (FUNUE || bexp ) x
and factorN x = ( (one is_lb|-- dexp‚Äì| one is_rb)||
FUNUE||
( Const_exp)||
( var_exp)||
one is_neg |-- factorN>> Neg
) x
and termN x = (factorN --(repeat (mult_or_div ‚Äì factorN))>>
(fn (e, op_fcts) => foldl (fn ((Delim ‚Äú*‚Äù , e2), e1) => Mult (e1, e2)
| ((Delim ‚Äú/‚Äù , e2), e1)=> Div (e1, e2)) e op_fcts)) x
and aexpN x = (termN --(repeat (add_or_minus ‚Äì termN)) >>
(fn (e, op_trms) => foldl (fn ((Delim ‚Äú+‚Äù , e2), e1) => Add (e1, e2)
| ((Delim ‚Äú-‚Äù , e2), e1) => Minus (e1, e2)) e op_trms)) x

and bfactorN x =((( aexpN ‚Äì cop ‚Äì aexpN)>>
(
fn ((a,Delim ‚Äú<‚Äù),b) => Lt (a,b)
| ((a,Delim ‚Äú<=‚Äù),b) => Le (a,b)
| ((a,Delim ‚Äú>‚Äù),b) => Gt (a,b)
| ((a,Delim ‚Äú<=‚Äù),b) => Ge (a,b)
| ((a,Delim ‚Äú==‚Äù),b) => Eq (a,b)
| ((a,Delim ‚Äú<>‚Äù),b) => Neq (a,b)
))

            || aexpN 
             || ( (one is_lb |-- bexpN --| one is_rb))
            || (((one tonot) |-- bfactorN) >> (fn n =>Not n))
          ) x
1
2
3
4
and btermN x = ((bfactorN ‚Äì repeat ((one(fn n => n =Delim ‚Äú&&‚Äù))-- bfactorN))
>>(fn (e,ops) => foldl (fn ((Delim ‚Äú&&‚Äù,e2),e1) => And (e1,e2)) e ops ))x

and bexpN x = ((btermN ‚Äì repeat ((one(fn n => n=Delim ‚Äú||‚Äù))‚ÄìbtermN)) >>
(fn (e,ops) => foldl (fn ((Delim ‚Äú||‚Äù,e2),e1) => Or (e1,e2)) e ops)) x
and FUNU1 x = (((one(is_ident) --| (one (is_lb)) --| one (is_rb)))>> (fn (Ident a) => Scall (a,[]))) x
and FUNU2 x = ((one(is_ident) ‚Äì (one (is_lb) |-- (shican) --| one (is_rb))) >> (fn (Ident a,(b,c)) => Scall (a,b::c))) x
and FUNU x = (FUNU1 || FUNU2) x
and FUNUE1 x = ((one(is_ident) --| (one (is_lb) --| one (is_rb)))>> (fn (Ident a) => Ecall (a,[]))) x
and FUNUE2 x = ((one(is_ident) ‚Äì (one (is_lb) |-- (shican) --| one (is_rb))) >> (fn (Ident a,(b,c)) => Ecall (a,b::c))) x
and FUNUE X = (FUNUE1 || FUNUE2) X
and shican x = (bexpN ‚Äì (repeat (one (fn n => n= Delim",") |-- bexpN))) x
and RETURN x = ((one (fn n=> n = Keyword ‚Äúreturn‚Äù) ‚Äì (one(is_lb)|-- bexpN --|one(is_rb)))>> (fn (a,b) => Return (SOME b))) x
and BLOCK1 x = ((one(is_LB) |‚ÄìSEQ --| one (is_RB)) >> (fn (a)=> Block ([],a))) x
and BLOCK2 x = ((one(is_LB) |‚Äì(repeat(one(is_ident))‚Äì(one(fn n=> n = Delim ‚Äú.‚Äù) |‚ÄìSEQ --| one (is_RB))))>>(fn (b,c) =>Block ((map (peal) b),c) )) x
and BLOCK x= (BLOCK1 || BLOCK2) x

}
