datatype exp = Const of int
| Var of string
| Add of exp * exp
| Minus of exp * exp
| Mult of exp * exp
| Div of exp * exp
| Neg of exp
|True
|False
|Lt of exp * exp
|Le of exp * exp
|Gt of exp * exp
|Ge of exp * exp
|Eq of exp * exp
|Neq of exp * exp
|Not of exp
|And of exp * exp
|Or of exp * exp
| Asgn of string * exp
| Seq of exp *exp
| While of exp * exp
| Ift of exp * exp
| Ife of exp * exp * exp
| Return of (exp option)
| Scall of (string * exp list)
| Ecall of (string * exp list)
| Block of ((string list) * exp)
| Fdecl of ((string * (string list)) * exp)
| Prog of exp list
*}

ML{*
fun is_lb (Delim ‚Äú(‚Äù) = true
|is_lb _ = false
fun is_rb (Delim ‚Äú)‚Äù) = true
|is_rb _ = false
}
ML{
val Const_exp = one(fn Number n => true)>> (fn Number n => Const n)
val var_exp = one(fn Ident n => true)>> (fn Ident n => Var n)

fun is_neg (Delim ‚Äú~‚Äù) = true
|is_neg _ = false
}
ML{
val mult_or_div =( one (fn n => n = Delim ‚Äú")
|| one (fn n => n = Delim ‚Äú/‚Äù))
val add_or_minus= one (fn n => n = Delim ‚Äú+‚Äù)
|| one (fn n => n = Delim ‚Äú-‚Äù)
fun is_neg (Delim ‚Äú~‚Äù) = true
|is_neg _ = false
fun foldl f e xs = foldr f e (rev xs)
}
ML{
fun factor x = ( (one is_lb|-- aexp‚Äì| one is_rb)||
( Const_exp)||
( var_exp)||
one is_neg |-- factor>> Neg
) x
and term x = (factor --(repeat (mult_or_div ‚Äì factor))>>
(fn (e, op_fcts) => foldl (fn ((Delim "‚Äù , e2), e1) => Mult (e1, e2)
| ((Delim ‚Äú/‚Äù , e2), e1)=> Div (e1, e2)) e op_fcts)) x
and aexp x = (term --(repeat (add_or_minus ‚Äì term)) >>
(fn (e, op_trms) => foldl (fn ((Delim ‚Äú+‚Äù , e2), e1) => Add (e1, e2)
| ((Delim ‚Äú-‚Äù , e2), e1) => Minus (e1, e2)) e op_trms)) x
}
